{
	"name": "book-search-backend",
	"version": "1.0.0",
	"description": "Express.js backend for book search application",
	"main": "dist/server.js",
	"type": "module",
	"scripts": {
		"watch": "nodemon --exec node --loader ts-node/esm src/server.ts",
		"build": "tsc",
		"start": "node dist/server.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
	},
	"keywords": [
		"express",
		"typescript",
		"mysql",
		"sequelize"
	],
	"author": "Mohammed Bhimjee",
	"license": "MIT",
	"dependencies": {
		"axios": "^1.10.0",
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"jsonwebtoken": "^9.0.2",
		"mysql2": "^3.9.7",
		"sequelize": "^6.37.3"
	},
	"devDependencies": {
		"@sequelize/cli": "^7.0.0-alpha.46",
		"@sequelize/mysql": "^7.0.0-alpha.46",
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/node": "^20.12.12",
		"nodemon": "^3.1.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	}
}
